		<setting id="autocleanwiz" type="bool" label="Wizard.Log automatisch reinigen: " visible="!eq(-6,false)" enable="!eq(-1,false)" default="true"/>
		<setting id="wizlogcleanby" type="enum" label="Wie soll der Wizard.Log gereinigt werden?" visible="!eq(-1,false)" values="Nach Tage|Nach Grosse|Nach Eintrage" default="1"/>
