		<setting id="includeincursion" type="bool" label="Incursion Cache DB: " default="true" enable="!eq(-6,false)" visible="!eq(-6,false)"/>
		<setting id="includeplacenta" type="bool" label="Placenta Cache DB: " default="true" enable="!eq(-7,false)" visible="!eq(-7,false)"/>
